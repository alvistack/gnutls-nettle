#!/usr/bin/make -f

SHELL := /bin/bash

override_dh_auto_configure:
	dh_auto_configure -- \
		--enable-static \
		--enable-shared \
		--enable-fat \
		--disable-documentation

override_dh_auto_build:
	dh_auto_build
	make nettle.info

override_dh_install:
	dh_install

override_dh_auto_test:

override_dh_auto_clean:

%:
	dh $@
